{
  "name": "G4 Debugger Analytics",
  "manifest_version": 3,
  "version": "1.0.1",
  "description": "Ferramenta de debugger analytics idealizada pelo time de Martech do G4 Educação.",
  "permissions": [
    "storage",
    "webRequest",
    "activeTab",
    "scripting"
  ],
  "action": {
    "default_icon": "logo.png"
  },
  "icons": {
    "16": "logo16.png",
    "48": "logo48.png",
    "128": "logo.png"
  },
  "background": {
    "service_worker": "background.js"
  },
  "host_permissions": ["<all_urls>"],
  "web_accessible_resources": [
    {
      "resources": [
        "widget.html",
        "widget.css",
        "widget.js",
        "events.js",
        "theme.js"
      ],
      "matches": ["<all_urls>"]
    }
  ]
}
